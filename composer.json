{
  "name": "zeroibc/laravel-u-editor",
  "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/zeroibc/laravel-u-editor"
        }
    ],
  "description": "UEditor for laravel5. Support i18n. UEditor is a Rich Text Web Editor From Baidu.",
  "keywords": [
    "laravel",
    "ueditor",
    "laravel-u-editor",
    "laravel5 web text editor",
    "stevenyangecho",
    "steven yang",
	"zeroibc",
	"OSS"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "zeroibc",
      "email": "zeroibc@qq.com"
    }
  ],

  "require": {
    "php": ">=5.4.0",
    "ext-fileinfo": "*",
    "illuminate/support": "5.*",
    "qiniu/php-sdk": "7.*",
	"aliyuncs/oss-sdk-php": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Stevenyangecho\\UEditor\\": "src/"
    }
  },
  "extra": {
        "laravel": {
            "providers": [
                "Stevenyangecho\\UEditor\\UEditorServiceProvider"
            ]
        }
    },	
  "minimum-stability": "dev",
  "prefer-stable": true
}
